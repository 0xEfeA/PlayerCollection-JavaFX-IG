<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<HBox id="VueEdition" fx:id="vueEdition" prefHeight="40.0" prefWidth="400.0" spacing="90.0" styleClass="vueEdition" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appli.controleurs.VueEditionController">
   <children>
      <Button id="boutonAjouter" fx:id="boutonAjouter" mnemonicParsing="false" onAction="#onAjouter" styleClass="bouton" stylesheets="@../style.css" text="Ajouter" textAlignment="CENTER" />
      <Region prefHeight="42.0" prefWidth="65.0" HBox.hgrow="ALWAYS" />
      <Button id="boutonEdition" fx:id="boutonEdition" mnemonicParsing="false" styleClass="bouton" stylesheets="@../style.css" text="Edition" />
      <Region prefHeight="42.0" prefWidth="65.0" HBox.hgrow="ALWAYS" />
      <Button id="boutonTrierpar" fx:id="boutonTrier" mnemonicParsing="false" styleClass="bouton" stylesheets="@../style.css" text="Trie par" />
   </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</HBox>
